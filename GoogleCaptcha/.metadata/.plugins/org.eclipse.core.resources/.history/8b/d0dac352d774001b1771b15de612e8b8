<%@page import = "java.util.List" %>
<%@page isELIgnored="false" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<html>
<body>
<h2>Hello World!</h2>
<%
String s = (String)request.getAttribute("name");
List<String> li = (List<String>)request.getAttribute("listt");
%>

<h1>Name is : 
<%=s %> </h1> 

<h4>
	<% for(String str : li)
		out.println(str);
	%> 
	
	Expression language : 
	
${name }
<hr>

<c:forEach var="l" items="listt">
<h3> ${l }</h3>

</c:forEach>
	
</h4>
</body>
</html>
